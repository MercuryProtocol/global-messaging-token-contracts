#!/Users/peekay/Desktop/code/global-messaging-token/venv3/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'ethereum-serpent==2.0.2','console_scripts','serpent'
__requires__ = 'ethereum-serpent==2.0.2'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('ethereum-serpent==2.0.2', 'console_scripts', 'serpent')()
    )
